        top: 50%;
        width: 100%;
        z-index: 3;
    }

    .sl-sobe-carousel-nav > button {
        background-color: #fff;
        border: none;
        box-shadow: 2px 0px 10px 2px rgba(0, 0, 0, 0.2);
        height: 100px;
        width: 45px;
        font-size: 25px;
        position: absolute;
        text-align: center;
        outline: none;
        transform: translate(0, -50%);
    }

    .sl-sobe-carousel-nav > button:hover {
        cursor: pointer;
    }
</style>

