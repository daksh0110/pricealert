    <script>
        P.when('A', 'jQuery', 'ready').execute(function (A, $){
            // We calculate Relative luminance https://www.w3.org/WAI/GL/wiki/Relative_luminance
            // If no good match we return 255 as thats value for white and nothing happens
            function lightness(rgb){
                if(rgb == null){
                    return 255;
                }
                const colors = rgb.match(/^rgb\((\d+),\s*(\d+),\s*(\d+)\)$/);
                if(typeof colors == "object" && colors.length == 4){
                    const r = colors[1];
                    const g = colors[2];
                    const b = colors[3];
                    return (0.2126*r + 0.7152*g+ 0.0722*b);
                }
                return 255;
            }
            let rgb = $("#sobe_d_b_2-carousel").css('backgroundColor');
            let lightnessOfBgColor = lightness(rgb);
            if (lightnessOfBgColor < 220){
                $("#sobe_d_b_2-carousel .sl-sobe-carousel-page-indicator .dot").addClass('dark-background');
            }
        });
    </script>


