        -webkit-overflow-scrolling: touch;
        padding-bottom: 10px;
        margin-bottom: -10px;
    }

    
    
    
    #sobe_d_b_8-carousel .sl-sobe-carousel-scroller {
        width: 33.333333333333336%;
    }
    
    
    

    

</style>

<script>
    P.when('A', 'jQuery', 'ready').execute(function (A, $) {
        const AUTO_SCROLL_TIMEOUT = 3000;
        const UNDO_INERTIAL_SCROLL_TIMEOUT = 200;

        var $container = $("#sobe_d_b_8-container");
        var $carousel = $container.find(".sl-sobe-carousel");
        var $viewport = $carousel.find(".sl-sobe-carousel-viewport");
        var $scroller = $carousel.find(".sl-sobe-carousel-scroller");

        var state = {
            userInputType: null,
            scrollTimeout: null,
            autoScrollTimeout: null,
            pageX: null,
            scrollRatio: 0,
            viewportScrollWidth: 0,
            viewportWidth: 0,
            isLazyLoadImagesTriggered: false,
            getViewportScrollLeft: function () {
                return this.scrollRatio * this.getViewportMaxScrollLeft();
            },
            getViewportMaxScrollLeft: function () {
                return this.viewportScrollWidth - this.viewportWidth;
            },
            getScrollerWidth: function () {
                return this.viewportWidth / 3.0;
            },
            getPaginationWidth: function() {
                var fullyVisibleSubCardsWidth = this.viewportWidth * 0.2054794520547945 * 4;
                var visibleMarginsWidth = this.viewportWidth * 0.018835616438356163 * 4;
                return fullyVisibleSubCardsWidth + visibleMarginsWidth;
            },
            updateViewportDimensions: function () {
                var rect = $viewport[0].getBoundingClientRect();
                state.viewportWidth = rect.width;
                state.viewportScrollWidth = $viewport[0].scrollWidth;
            }
        };

        function onResize() {
            if (state.animating) {
                return;
            }
            state.animating = true;
            requestAnimationFrame(onResizeAnimation);
        }

        function onResizeAnimation() {
            state.updateViewportDimensions();

            if (state.scrollable) {
                $viewport.scroll();
            }

            

            state.animating = false;
        }

        function updatePageIndicator() {
            var activePageNumber = Math.trunc(state.getViewportScrollLeft()) / state.viewportWidth;
            if (activePageNumber == 0) {
                activePageNumber = 1;
            } else if (activePageNumber === Math.ceil(activePageNumber)) {
                activePageNumber++;
            } else {
                activePageNumber = Math.ceil(activePageNumber) + 1;
            }

            if (!state.isLazyLoadImagesTriggered) {
                const $wrapper = $container.find(".sl-sobe-carousel-sub-card-image");
                A.loadDescendantImagesManually($wrapper);
                state.isLazyLoadImagesTriggered = true;
            }

            

            $carousel.find(".sl-sobe-carousel-page-indicator > .active").removeClass("active");
            
            $carousel.find(".sl-sobe-carousel-page-indicator").children().eq(activePageNumber - 1).addClass("active");
            
            state.activePageNumber = activePageNumber;
        }

        function onUserInputAction(event) {
            if (state.userInputType != null) { return; }
            state.userInputType = event.type === "touchstart" ? "touch" : "mouse";
            if (state.userInputType === "mouse" && state.scrollable) {
                var $nav = $carousel.find(".sl-sobe-carousel-nav");
                $nav.find(".sl-sobe-carousel-goto-prevpage").click({type: "prev"}, onNavButtonClick);
                $nav.find(".sl-sobe-carousel-goto-nextpage").click({type: "next"}, onNavButtonClick);
                $scroller.mousedown(onDragScrollerStart);
            }
            
            $carousel.attr("userInputType", state.userInputType);
        }

        function onViewportScroll() {
            if (state.viewportScrolling) {
                return;
            }
            state.viewportScrolling = true;
            requestAnimationFrame(onViewportScrollAnimation);
        }

        function onViewportScrollAnimation() {
            state.scrollRatio = $viewport[0].scrollLeft / state.getViewportMaxScrollLeft();
            $scroller.css({
                "left": state.scrollRatio * (state.viewportWidth - state.getScrollerWidth())
            });

            var scrollPct = state.scrollRatio * 100;
            if (scrollPct < 0.1) {
                scrollPct = 0;
            } else if (scrollPct > 99.9) {
                scrollPct = 100;
            }
            $carousel.attr("scrollPct", scrollPct);

            if (state.userInputType === "touch") {
                $carousel.addClass("scrolling");
                if (state.scrollTimeout != null) {
                    clearTimeout(state.scrollTimeout);
                }
                state.scrollTimeout = setTimeout(function () {
                    $carousel.addClass("scrolling");
                }, 1000);
            }

            
            
            

            if (state.showPageIndicator) {
                updatePageIndicator();
            }
            state.viewportScrolling = false;
        }

        function onDragScrollerStart(event) {
            $carousel.addClass("scrolling");
            $(document).bind("mousemove", onDragScroller);
            $(document).one("mouseup", onDragScrollerEnd);
            state.pageX = event.pageX;
        }

        function onDragScroller(event) {
            var deltaScrollRatio = (event.pageX - state.pageX) / state.viewportWidth;
            $viewport[0].scrollLeft += deltaScrollRatio * state.viewportScrollWidth;
            state.pageX = event.pageX;
        }

        function onDragScrollerEnd(event) {
            $carousel.removeClass("scrolling");
            $(document).unbind("mousemove", onDragScroller);
        }

        function onNavButtonClick(event) {
            event.preventDefault();
            var scrollLeft = state.getViewportScrollLeft();
            if (event.data.type === "prev") {
                scrollLeft = Math.max(scrollLeft - state.getPaginationWidth(), 0);
            } else if (event.data.type === "next") {
                scrollLeft = Math.min(scrollLeft + state.getPaginationWidth(), state.getViewportMaxScrollLeft());
            }
            

            
            $viewport.animate({
                scrollLeft: scrollLeft
            }, onViewportScroll);
        }

        function onPageIndicatorDotClick(event){
            event.preventDefault();
            let pageNumber = event.target.getAttribute("data-page-number");

            

            scrollToPage(pageNumber);

            
        }

        function scrollToPage(pageNumber){
            $viewport.animate({
                scrollLeft: (pageNumber - 1) * state.getPaginationWidth()
            }, onViewportScroll);
        }

        function autoScroll() {
            let scrollLeft = state.getViewportScrollLeft();
            scrollLeft = Math.min(scrollLeft + state.getPaginationWidth(), state.getViewportMaxScrollLeft());
            
            $viewport.animate({
                scrollLeft: scrollLeft
            }, 400, onViewportScroll);
            state.autoScrollTimeout = setTimeout(autoScroll, AUTO_SCROLL_TIMEOUT);
        }

        function copyFirstAndLastPage() {
            const $viewPortRowInner = $viewport.find(".sl-sobe-carousel-viewport-row-inner");
            const $firstPage = $viewPortRowInner.find("li.sl-sobe-carousel-sub-card").first().clone();
            const $lastPage = $viewPortRowInner.find("li.sl-sobe-carousel-sub-card").last().clone();
            $firstPage.prop("id", $firstPage.attr("id") + "_clone");
            $firstPage.find('a').contents().unwrap();
            $lastPage.prop("id", $lastPage.attr("id") + "_clone");
            $lastPage.find('a').contents().unwrap();
            $viewPortRowInner.prepend($lastPage);
            $viewPortRowInner.append($firstPage);
        }

        function scrollToClosestPage() {
            const activePageNumber = Math.round(state.getViewportScrollLeft() / state.viewportWidth);
            $viewport.scrollLeft(activePageNumber * state.getPaginationWidth());
        }

        function init() {
            $viewport.bind("mousemove touchstart", onUserInputAction);
            $viewport.scroll(onViewportScroll);
            $(window).resize(function () {
                setTimeout(onResize, 10);
            });
            state.updateViewportDimensions();

            state.scrollable = true;
            state.showPageIndicator = false;
            if (state.scrollable) {
                if(state.showPageIndicator){
                    $carousel.find(".sl-sobe-carousel-page-indicator > .dot").click(onPageIndicatorDotClick);
                }
                

                

                $carousel.addClass("scrollable");
                $viewport.scroll();
            }
            $carousel.css("visibility", "visible");
        }

        init();
    });
</script>



<div id="sobe_d_b_8-container">
    <div id="sobe_d_b_8-carousel" class="sl-sobe-carousel" scrollPct="0" userInputType="touch">
        
            
                
                
                    





<style>
    .sl-sobe-carousel-header {
        font-size: 0;
        line-height: normal;
    }

    

    .sl-sobe-carousel-header > h2,
    .sl-sobe-carousel-header > a {
        display: inline-block;
        line-height: normal;
    }

    .sl-sobe-carousel-header > h2 {
        /* Font size ranges from 18px (at 1000px width) to 22px (at 1500px width) */
        font-size: calc(0.8vw + 10px);
        padding: 0;
    }

    .sl-sobe-carousel-header > a {
        margin-left: 10px;
        font-size: 14px;
    }

    @media (max-width: 1000px) {
        .sl-sobe-carousel-header > h2 {
            font-size: 18px !important;
        }
    }

    @media (min-width: 1500px) {
        .sl-sobe-carousel-header > h2 {
            font-size: 22px !important;
        }
    }
</style>




