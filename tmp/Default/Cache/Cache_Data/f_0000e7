        position: relative;
        width: 100%;
    }

    #sobe_d_b_13-carousel {
    
    
    
        background-color: #fff;
    
    
        padding: 24px 20px 24px 20px;
        height: 100%;
        width: 100%;
    }

    #sobe_d_b_13-carousel-viewport-container {
        position: relative;
    }

    .sl-sobe-carousel-viewport {
        -ms-overflow-style: none;
        height: 100%;
        font-size: 0;
        overflow-x: auto;
        overflow-y: hidden;
        white-space: nowrap;
        width: 100%;
    }

    #sobe_d_b_13-carousel .sl-sobe-carousel-viewport-row {
        height: 100.0%;
        
    }

    .sl-sobe-carousel-viewport-row {
        margin: 0;
        padding: 0;
    }

    .sl-sobe-carousel-viewport-row-inner {
        height: 100%;
        list-style: none;
        margin: 0;
        padding: 0;
        width: 100%;
    }

    .sl-sobe-carousel-sub-card {
        display: inline-block;
        max-height: 100%;
        height: 100%;
        margin-left: 20px;
