    }

    .sl-sobe-carousel-viewport {
        -ms-overflow-style: none;
        height: 100%;
        font-size: 0;
        overflow-x: auto;
        overflow-y: hidden;
        white-space: nowrap;
        width: 100%;
    }

    #sobe_d_b_2-carousel .sl-sobe-carousel-viewport-row {
        height: 100.0%;
        
    }

    .sl-sobe-carousel-viewport-row {
        margin: 0;
