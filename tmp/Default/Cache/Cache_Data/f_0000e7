    <button class="sl-sobe-carousel-goto-prevpage" style="left:0;" aria-hidden="true" tabindex="-1">&#10094;</button>
    <button class="sl-sobe-carousel-goto-nextpage" style="right: 0;" aria-hidden="true" tabindex="-1">&#10095;</button>
