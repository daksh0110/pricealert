  <script>function HveCountdownTimer(e){this.requiredConstructWidgetArguments=["EndDate","WidgetContainerId","TemplateType"],this.TemplateTypes={ShortSWM:{templateName:"short-swm-primeday",borderRadius:2,fontSize:22,fontFamily:"Amazon Ember",xLocation:190,yLocation:3,padding:"2px",timeMargin:"2px 2px 0px 2px",colonMargin:"2px 2px 0px 2px",useLabels:!1,lineHeight:"25px"},ShortPrimeday:{templateName:"short-primeday",borderRadius:3,fontSize:27,labelFontSize:11,fontFamily:"Amazon Ember",xLocation:130,yLocation:25,padding:"0.1em",timeMargin:"0 0.12em 0 0",colonMargin:"0 0.12em 0 0",useLabels:!0,lineHeight:"normal"},TallPrimeday:{templateName:"tall-primeday",borderRadius:5,fontSize:46,labelFontSize:14,fontFamily:"Amazon Ember",xLocation:15,yLocation:132,padding:"0.15em",timeMargin:"0 0.1em 0 0",colonMargin:"0 0.1em 0 0",useLabels:!0,lineHeight:"normal"}};var i=this;this.timeUnits=["hours","minutes","seconds"],this.timeUnitProperties={hours:{name:"hours",unitInMilliseconds:36e5,formatFunction:function(t){return i.appendLeadingZeros(t,2)}},minutes:{name:"minutes",unitInMilliseconds:6e4,formatFunction:function(t){return i.appendLeadingZeros(t,2)}},seconds:{name:"seconds",unitInMilliseconds:1e3,formatFunction:function(t){return i.appendLeadingZeros(t,2)}}};try{if(!e)throw"ConstructWidgetArgs is not defined in HveCountdownTimer constructor";var t=this.validateConstructWidgetRequest(e);if(0<t.length)return void this.handleConstructWidgetArgsError(e,t,"Construct Widget");if(this.widget=document.getElementById(e.WidgetContainerId),!this.widget)return void this.handleConstructWidgetArgsError(e,["Widget Container not found"],"Construct Widget");this.widget.style.height="100%",this.widget.style.width="100%",this.isHex(e.WidgetBackgroundColor)&&(this.widget.style.backgroundColor=e.WidgetBackgroundColor),this.timeUnitDivs={},this.timeUnitLabels=this.getTimeUnitLabels(e),this.endUnit="seconds",this.buildHtmlContent(e),this.paintWidget(new Date(e.EndDate).getTime()),this.removeOverflowi