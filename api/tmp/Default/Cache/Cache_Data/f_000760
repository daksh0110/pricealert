ons.ms || 25; const el = options.el || window; const throttledFn = this.throttle(fn, ms); el.addEventListener(eventName, throttledFn); return function() {el.removeEventListener(eventName, throttledFn); } }, /** * Returns a throttled version of the function. * @param {function} fn - the function to be throttled * @param {number} delay - the amount of time to delay between function calls * @returns {function} - the throttled function */ throttle: function(fn, delay) {let timeoutId; return function() {const args = arguments; const context = this; if (!timeoutId) {timeoutId = setTimeout(function() {fn.apply(context, args); timeoutId = null; }, delay) } } }, /** * Fires viewable impression and log info in case of error. * @param {string} pixel - the function to be throttled */ emitViewMetrics: function(pixel) {const timeInView = Date.now() - this.startTimeInView; const payload = JSON.stringify({v: {def: 'iab', event: 'VIEWED', p: Math.round(this.percentageViewed * 100), t: parseFloat((timeInView / 1000).toFixed(3)) }, programType: 'SBC', }); const pixelUrlWithPixelType = pixel.substr(-1) === '/' ? pixel + 'v/' : pixel + '/v/'; if (typeof window.fetch === 'function') {fetch(pixelUrlWithPixelType + encodeURI(payload), {method: 'POST', mode: 'no-cors', headers: {'Content-Type': 'application/json', }, body: payload, }) .then((res) => {if (!res.ok && res.status !== 0) {throw Error(res.status + ' ' + res.statusText); } if (window.ue && window.ue.count) {window.ue.count('sbxGwentClientViewPixelCallSuccess', 1); } }) .catch((e) => {if (window.ue && window.ue.count) {window.ue.count('sbxGwentClientViewPixelCallError', 1); } }); } else {/* Handle older browsers such as IE */ var xhr = new XMLHttpRequest(); xhr.open('POST', pixelUrlWithPixelType + encodeURI(payload)); xhr.setRequestHeader('Content-Type', 'application/json'); xhr.onreadystatechange = function() {if (xhr.readyState === 4) {if (xhr.status !== 200) {if (window.ue && window.ue.count) {window.ue.count('sbxGwentClientView